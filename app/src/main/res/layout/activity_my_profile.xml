<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageButton
        android:id="@+id/imageButton"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="130dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/firstName"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="20dp"
        android:ems="10"
        android:textSize="15dp"
        android:text="First Name" />

    <TextView
        android:id="@+id/lastName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="Last Name"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:layout_below="@+id/firstName"/>

    <TextView
        android:id="@+id/gmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="Email"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:layout_below="@+id/lastName"/>

    <TextView
        android:id="@+id/DOB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="Tempat/Tanggal Lahir"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:layout_below="@+id/gmail"/>

    <TextView
        android:id="@+id/University"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="University"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:layout_below="@+id/DOB"/>

    <TextView
        android:id="@+id/universityYear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="Year"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:layout_below="@+id/University"/>

    <TextView
        android:id="@+id/universityMajor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="Major"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:layout_below="@+id/universityYear"/>

    <TextView
        android:id="@+id/Gender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="Gender"
        android:layout_marginTop="10dp"
        android:textSize="15dp"
        android:layout_below="@+id/universityMajor"/>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="180dp"
        android:text=":" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="180dp"
        android:text=":"
        android:layout_below="@+id/textView1"/>/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="180dp"
        android:text=":"
        android:layout_below="@+id/textView2"/>/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="180dp"
        android:text=":"
        android:layout_below="@+id/textView3"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="180dp"
        android:text=":"
        android:layout_below="@+id/textView4"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="180dp"
        android:text=":"
        android:layout_below="@+id/textView5"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="180dp"
        android:text=":"
        android:layout_below="@+id/textView6"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="180dp"
        android:text=":"
        android:layout_below="@+id/textView7"/>

    <EditText
        android:id="@+id/editFirstname"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="200dp"
        android:textSize="15dp"
        android:layout_marginTop="190dp"
        android:ems="100"
        android:text="" />

    <EditText
        android:id="@+id/editLastname"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="200dp"
        android:textSize="15dp"
        android:layout_marginTop="220dp"
        android:ems="100"
        android:text="" />

    <EditText
        android:id="@+id/editGmail"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="200dp"
        android:textSize="15dp"
        android:layout_marginTop="250dp"
        android:ems="100"
        android:text="" />

    <EditText
        android:id="@+id/editDOB"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="200dp"
        android:textSize="15dp"
        android:layout_marginTop="280dp"
        android:ems="100"
        android:text="" />

    <EditText
        android:id="@+id/editUniversity"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="200dp"
        android:textSize="15dp"
        android:layout_marginTop="310dp"
        android:ems="100"
        android:text="" />

    <EditText
        android:id="@+id/editUniversityYear"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="200dp"
        android:textSize="15dp"
        android:layout_marginTop="340dp"
        android:ems="100"
        android:text="" />
    <EditText
        android:id="@+id/editUniversitymajor"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="200dp"
        android:textSize="15dp"
        android:layout_marginTop="370dp"
        android:ems="100"
        android:text="" />
    <EditText
        android:id="@+id/editGender"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="200dp"
        android:textSize="15dp"
        android:layout_marginTop="400dp"
        android:ems="100"
        android:text="" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="480dp"
        android:ems="10"
        android:text="Upload Your CV"
        android:textSize="15dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="460dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_upload" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="480dp"
        android:ems="10"
        android:text="History"
        android:textSize="15dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="460dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_recent_history" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="530dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_save" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="530dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_close_clear_cancel" />

</RelativeLayout>